modules = ["web", "python-3.11", "nodejs-20"]
[agent]
expertMode = true
integrations = ["javascript_openai_ai_integrations:1.0.0"]

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Server"

[[workflows.workflow]]
name = "Server"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python server.py"
waitForPort = 5000

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 8001
externalPort = 4200

[[ports]]
localPort = 34371
externalPort = 5173

[[ports]]
localPort = 34821
externalPort = 6000

[[ports]]
localPort = 38461
externalPort = 6800

[[ports]]
localPort = 38493
externalPort = 3002

[[ports]]
localPort = 39501
externalPort = 3001

[[ports]]
localPort = 43287
externalPort = 5000

[[ports]]
localPort = 43633
externalPort = 3000

[[ports]]
localPort = 45007
externalPort = 3003

[deployment]
deploymentTarget = "autoscale"
run = ["python", "server.py"]
